{
  "name": "lwac-apt-repo",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "npm run lint:eslint & npm run lint:stylelint",
    "lint:eslint": "eslint . --ext .ts,.tsx,.js,.jsx --cache",
    "lint:eslint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --cache --fix",
    "lint:stylelint": "stylelint 'src/**/*.{css,scss}' --cache",
    "prettier": "prettier --check \"**/*.{ts,tsx,jsx,js,css,scss}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx,jsx,js,css,scss}\""
  },
  "dependencies": {
    "@mantine/code-highlight": "=7.10.0",
    "@mantine/core": "=7.10.0",
    "mantine-datatable": "^7.9.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-mantine": "3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.38",
    "postcss-merge-rules": "^7.0.0",
    "postcss-preset-mantine": "1.15.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "prop-types": "^15.8.1",
    "stylelint": "^16.2.1",
    "stylelint-config-standard-scss": "^13.0.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.2",
    "vite": "^5.2.10",
    "vite-tsconfig-paths": "^4.3.1"
  }
}
